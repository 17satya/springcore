<?xml version="1.0" encoding="UTF-8"?>
<beans  xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
       http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
		http://www.springframework.org/schema/context
		http://www.springframework.org/schema/context/spring-context-4.0.xsd">
		
		<bean id = "addr" class = "com.spring.springcoreprograms.SI_Address">
		<property name="hno" value = "21"></property>
		<property name="city" value = "hyderabad"></property>
		<property name="state" value = "telangana"></property>
		</bean>
		<bean id = "employee" class = "com.spring.springcoreprograms.SI_Employee">
		<property name="name" value = "venkat"></property>
		<property name="id" value = "101"></property>
		<property name="address" ref = "addr" ></property>
		</bean>
</beans>