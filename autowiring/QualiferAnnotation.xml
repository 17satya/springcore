<?xml version="1.0" encoding="UTF-8"?><beans  xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
       http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
		http://www.springframework.org/schema/context
		http://www.springframework.org/schema/context/spring-context-4.0.xsd">
	
	<context:annotation-config/>
<bean id = "student1" class = "com.spring.springcore.autowiring.Student">
<property name="name" value = "venkat reddy"></property>
<property name="age" value = "22"></property>
<property name="height" value = "5.6"></property>
</bean>

<bean id = "student2" class = "com.spring.springcore.autowiring.Student">
<property name="name" value = "rakesh"></property>
<property name="age" value = "22"></property>
<property name="height" value = "5.12"></property>
</bean>
<bean id = "details" class = "com.spring.springcore.autowiring.Details"></bean>
</beans>